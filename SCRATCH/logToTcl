#!/usr/bin/perl
my $noOfArg = @ARGV;
my $logFile;
my $tclFile;
if($noOfArg < 4 || $ARGV[0] eq "-h" || $ARGV[0] eq "-help"){
   print"Usage ./logToTcl -log <log file> -tcl <tcl file>\n";
   exit;
}else{
   for(my $i=0; $i<$noOfArg; $i++){
      if($ARGV[$i] eq "-log"){$logFile = $ARGV[$i+1];} 
      if($ARGV[$i] eq "-tcl"){$tclFile = $ARGV[$i+1];} 
   }
   open(WRITE,">$tclFile");
   open(READ, "$logFile");
   while(<READ>){
      chomp($_);
      if( $_ =~ /^#<CMD>\s*(.*)/ ) { print WRITE"$1\n";}
   }
   close(READ);
   close(WRITE);
}
