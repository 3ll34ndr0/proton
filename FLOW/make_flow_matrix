
sub dbinitEverthing {
print "MSG : 000 : initilizing Physical Library Database\n" if ($DEBUG > 5);
$PLIB_IN = 0;
print "MSG : 000 : initilizing Timing   Library Database\n" if ($DEBUG > 5);
$TLIB_IN = 0;
