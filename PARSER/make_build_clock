
sub do_trace_clocks {

my $noOfArguments = @_;

if($noOfArguments < 1  || $_[0] eq '-h'){ print "Usage : do_trace_clocks -pin <pinName>\n";}
if($noOfArguments > 3 || $_[0] eq '-h'){ print "Usage : do_trace_clocks -pin <pinName>\n";}

$referenceTheNetsDataBase = \%NET_ALREADY;
$referenceTheComponentDataBase = \%COMP_ALREADY;






}#sub do_trace_clocks

1;
