package TimingLibRelatedPinCondDB ;

sub new {
my $dd = {
          TTYPE => "UND",
          TSENSE => "UND",
          SDFCOND => "UND",
          RISE_DELAY => [],
          FALL_DELAY => [],
          RISE_DELAY_INDEX => [],
          FALL_DELAY_INDEX => [],
          FALL_POWER => [],
          RISE_POWER => [],
          FALL_POWER_INDEX => [],
          RISE_POWER_INDEX => [],
          };
bless $dd, 'TimingLibRelatedPinCondDB';
return $dd;
}# sub new

1;
