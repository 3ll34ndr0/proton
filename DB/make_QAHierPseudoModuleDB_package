package PseudoModuleDB;

# TOP : is the module top module or not 0 : not top module
#                                        1 : is top module
# PM : Parent Module Name
sub new {
my $dd = {
          TOP => 0,
          PM => "UND",
          Master => "UND",
          };
bless $dd, 'PseudoModuleDB';
return $dd;
}# sub new
