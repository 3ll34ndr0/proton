package RTLDB_REGISTER_ASSIGN;


sub new {
my $dd = {
          assign_string => "",
          lhs_expression => "",
          rhs_expression => "",
         };
bless $dd, 'RTLDB_REGISTER_ASSIGN';
return $dd;
}#sub new
#--------------------------------------------------------------------------------------------#
sub dbRTLDB_REGISTER_ASSIGN_Set_assing_string {
my ($self,$temp_assign_string) = @_;
$self->{assign_string} = $temp_assign_string;
}#sub dbRTLDB_REGISTER_ASSIGN_Set_assing_string

sub dbRTLDB_REGISTER_ASSIGN_Get_Exp_String {
my ($self) = @_;
my $temp_assign_string = $self->{assign_string};
return($temp_assign_string);
}#sub dbRTLDB_REGISTER_ASSIGN_Get_Exp_String
#--------------------------------------------------------------------------------------------#
sub dbRTLDB_REGISTER_ASSIGN_Set_lhs_expression {
my ($self,$temp_lhs_expression) = @_;
$self->{lhs_expression} = $temp_lhs_expression;
}#sub dbRTLDB_REGISTER_ASSIGN_Set_lhs_expression

sub dbRTLDB_REGISTER_ASSIGN_Get_lhs_expression {
my ($self) = @_;
my $temp_lhs_expression = $self->{lhs_expression};
return($temp_lhs_expression);
}#sub dbRTLDB_REGISTER_ASSIGN_Get_lhs_expression
#--------------------------------------------------------------------------------------------#
sub dbRTLDB_REGISTER_ASSIGN_Set_rhs_expression {
my ($self,$temp_rhs_expression) = @_;
$self->{rhs_expression} = $temp_rhs_expression;
}#sub dbRTLDB_REGISTER_ASSIGN_Set_rhs_expression

sub dbRTLDB_REGISTER_ASSIGN_Get_rhs_expression {
my ($self) = @_;
my $temp_rhs_expression = $self->{rhs_expression};
return($temp_rhs_expression);
}#sub dbRTLDB_REGISTER_ASSIGN_Get_rhs_expression
1;
